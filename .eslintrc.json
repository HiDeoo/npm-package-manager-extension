{
  "extends": "@hideoo",
  "overrides": [
    {
      "files": ["*.svelte"],
      "processor": "svelte3/svelte3",
      "rules": {
        "no-undef": "off",
        "@typescript-eslint/no-use-before-define": "off",

        "import/no-mutable-exports": "off"
      }
    }
  ],
  "plugins": ["svelte3"],
  "settings": {
    "svelte3/typescript": true
  }
}
